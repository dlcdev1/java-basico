module.exports = function () {

  'ngInject';

  const details = function(data = {}) {
    const checkData = this.validateProp(data);

    this.submissionName = checkData('submissionName');
    this.submissionId = checkData('submissionId');
    this.campaignName = checkData('campaignName');
    this.campaignId = checkData('campaignId');
    this.campaignUrl = checkData('campaignUrl');
    this.sfdcSyncMessage = checkData('sfdcSyncMessage');
    this.campaignOwner = checkData('campaignOwner');
    this.startDate = checkData('startDate');
    this.endDate = checkData('endDate');
    this.launchDate = checkData('launchDate');
    this.campaignYear = checkData('campaignYear');
    this.type = checkData('type');
    this.financeChannel = checkData('financeChannel');
    this.campaignReusable = checkData('campaignReusable');
  };

  details.prototype = {
    validateProp(obj) {
      return (key) => obj && obj[key] ? obj[key] : null;
    }
  };

  return details;
};
