package br.com.java.ciant;

public class Array {

	public static void main(String[] args) {
		int [] array = new int [4];
		int[][] array2 = new int [5][5];
		
	
		
		array [0] = 100;
		array [1] = 85;
		array [2] = 20;
		array [3] = 35;
		
		array2 [0][0] = 100;
		
		System.out.println(array[0] + "- "+ array[1]);
		System.out.println(array2[0]+[0]);
		


	}

}
