package br.com.java.ciant;

public class ControleDeFlusoFor {

	public static void main(String[] args) {
		char nome [] = new char[6];
		nome[0] = 'd';
		nome[1]  = 'a';
		nome[2] = 'v';
		nome[3] = 'i';
		nome[4] = 'd';
		nome[5] ='i';
		for (int i; i <nome[i]; i++) {
			System.out.println(nome[i]);
		}

	}

}
