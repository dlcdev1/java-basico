package br.com.java.ciant;

public class Array {

	public static void main(String[] args) {
		int [] array = new int [10];
		char [] nome1 = new char [2];
		String nome = new String ("texto"); 

	
		nome1 [0] = 2;
		array [0] = 100;
		array [1] = 85;
		array [2] = 20;
		array [3] = 35;
		
		System.out.println(array[0] + "- "+ array[1]);
		System.out.println(nome1[0]);
	


	}

}
