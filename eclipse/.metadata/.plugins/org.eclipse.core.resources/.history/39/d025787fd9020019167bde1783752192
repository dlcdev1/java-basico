package br.com.aula19.arrays;

public class Exer04 {

	public static void main(String[] args) {
		/*Criar um vetor A com 25 elementos inteiros. Construir um vetor B de mesmo tamanho
		 * , sendo que cada elemento do vetoB dever√° ser a raiz quadrada do respectivo elemento de A,
		 * ou seja: B[I] = sqrt(A[I])*/
		
		int [] vetorA = new int[15];
		int [] vetorB = new int [vetorA.length];
		
		int rodar = 15;
		
		System.out.print("Vetor A=");
		for (int i=0; i < rodar; i++) {
			vetorA[i]=i;
			System.out.print(vetorA[i]);
		}
		System.out.println();
		System.out.print("Vetor B=");
		
		for (int i=0; i < 15; i++) {
			vetorB[i] = vetorA[i];
			System.out.println(vetorB[i]);
		}

	}

}
