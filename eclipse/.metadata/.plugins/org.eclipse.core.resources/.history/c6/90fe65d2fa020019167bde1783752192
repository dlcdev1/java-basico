package br.com.aula19.arrays;

import java.util.Scanner;

public class Exer12 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);

		int[] vetorA = new int[10];
		int[] soma = new int[vetorA.length];
		int[] result = new int[vetorA.length];

		for (int i = 0; i < vetorA.length; i++) {
			System.out.println("Informe o " + i + "° número");
			vetorA[i] = scan.nextInt();
		}
		System.out.println();

		System.out.println("A soma do VetorA");
		for (int i = 0; i < vetorA.length; i++) {
				vetorA[i] = i;
				vetorA[i] = vetorA[i];
				soma[i] = soma[i] + vetorA[i];
				System.out.println("Resultado dos números é" + soma[i]);
			
			
		}
		
	}
}
