package br.com.java.ciant;

public class Exercicio1 {

	public static void main(String[] args) {
		int number = 0, number1=0, number2=0, number3=0;
		number = 3-2-1+2+1+3;
		number1 = 2*3 - 4*5;
		number2 = 2+6 -3/7*9;
		number3 = 3%4-8;
		System.out.println("Primeiro resultado: "+ number);
		System.out.println("Segundo resultado: " + number1);
		System.out.println("Terceiro resultado: " + number2);
		System.out.println("Quarto resultado:  " + number3);

	}

}
