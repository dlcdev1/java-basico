package br.com.aula27;

public class Lampada {
	String modelo;
	String tensao;
	int potencia;
	String cor;
	String tipoLuz;
	int garantiaMeses;
	String[]tipos;
	boolean tipoAbajur;
	
	boolean ligada;
	boolean desligar;
	
	void ligar() {
		ligada = true;
	}
	
	void desligar() {
		desligar = false;
	}
	

	

}
